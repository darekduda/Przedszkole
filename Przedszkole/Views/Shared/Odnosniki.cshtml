@model IEnumerable<PrzedszkoleData.Data.CMS.Parametr>

<ul>


    <li><a class="nav-link scrollto @(ViewContext.RouteData.Values["action"].ToString() == "Index" ? "active" : "")" asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
    @foreach (var item in Model)
    {
        @if (item.Nazwa == "Home") continue;
        <li><a class="nav-link text-dark" asp-controller="Home" asp-action="Index" asp-route-id="@item.Nazwa">@item.Nazwa</a></li>

    }
    <li><a class="nav-link scrollto @(ViewContext.RouteData.Values["action"].ToString() == "ONas" ? "active" : "")" asp-area="" asp-controller="Home" asp-action="ONas">O Nas</a></li>
    <li><a class="nav-link scrollto @(ViewContext.RouteData.Values["action"].ToString() == "Zajecia" ? "active" : "")" asp-area="" asp-controller="Home" asp-action="Zajecia">Zajęcia</a></li>
    <li><a class="nav-link scrollto @(ViewContext.RouteData.Values["action"].ToString() == "Cennik" ? "active" : "")" asp-area="" asp-controller="Home" asp-action="Cennik">Cennik</a></li>
    <li><a class="nav-link scrollto @(ViewContext.RouteData.Values["action"].ToString() == "Portfolio" ? "active" : "")" href="#portfolio">Portfolio</a></li>
    <li><a class="nav-link scrollto @(ViewContext.RouteData.Values["action"].ToString() == "Kadra" ? "active" : "")" asp-area="" asp-controller="Home" asp-action="Kadra">Kadra</a></li>
    <li><a class="nav-link scrollto @(ViewContext.RouteData.Values["action"].ToString() == "Kontakt" ? "active" : "")" asp-area="" asp-controller="Home" asp-action="Kontakt">Kontakt</a></li>
</ul>